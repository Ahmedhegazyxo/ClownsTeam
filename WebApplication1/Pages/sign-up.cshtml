        <form class="sign-up-form1" method="post">
          <div class="sign-up-container2">
            <div class="sign-up-container3">
              <span class="sign-up-p">First name</span>
              <span class="sign-up-p1">Last name</span>
              <span class="sign-up-text1">Email</span>
              <span class="sign-up-p2">Password</span>
              <span class="sign-up-p3">Confirm Password</span>
              <span class="sign-up-p4">Address</span>
              <span class="sign-up-p5">Phone Number</span>
              <span class="sign-up-p6">Birth - date</span>
            </div>
            <div class="sign-up-container4">
              <input  type="text" placeholder="first name" class="input" name="first-name" />
              <input  type="text" placeholder="last name" class="input" name="last-name"/>
              <input
                type="email"
                required
                placeholder="email address"
                class="sign-up-textinput04 input"
                  runat="server"
                 name="txtEmail"/>
              <input
                type="password"
                required
                placeholder="password"
                class="input"
                 name="password"
              />
              <input
                type="password"
                required
                placeholder="confirm password"
                class="input"
                 name="confirmPassword"
              />
              <input type="text" required placeholder="Address" class="input"
              name="address"
              />
              <input type="tel" placeholder="Phone number" class="input"
              name="phone"/>
              <input type="date" placeholder="DOB" class="input"
              name="birthdate"/>
            </div>
          </div>
          <br> <br>
          <div align="center"><input type="submit" class="button" value="Sign Up" /></div>

